<h2><%= l(:label_meeting) %> #<%= @meeting.id %></h2>
<%= labelled_tabular_form_for @meeting, :url => {:controller => 'meetings', :action => 'update'}, :html => {:id => 'meeting-form', :method => :put} do |f| -%>
